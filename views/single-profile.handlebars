<div class="row">
  <div class="col-md-6">
    <div class="card">
      <div class="card-body">
        <img src="{{profile.photo}}" alt="">
        <p class="lead">{{profile.user.name}}</p>
        <p>{{profile.city}}, {{profile.country}}</p>
        <p>{{profile.email}}</p>
        <p>{{profile.phone}}</p>

      </div>
    </div>


  </div>
  <div class="col-md-6">
    <div class="card">
      <div class="card-body">
        <p>{{profile.about}}</p>
        <p>{{profile.experience}}</p>
        <p>{{profile.hobbies}}</p>
      </div>
    </div>
  </div>
</div>